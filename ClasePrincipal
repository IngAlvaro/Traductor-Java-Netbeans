    private void btntraducirActionPerformed(java.awt.event.ActionEvent evt) {                                            
        
            // TODO add your handling code here:
           Translate.setHttpReferrer("http://code.google.com/apis/language/translate/v2/getting_started.html");
           Translate.setKey("AIzaSyDhwlBP6OcjjvPqtBzS6Ux1rw8BQE3C5DA");
            
            
         try {   
            //GoogleAPI.validateReferrer(); 
            String peticion = (String) txtArea1.getText();
            // try{
            
           String valorResultado = Translate.translate(peticion,Language.SPANISH,Language.ENGLISH);
            txtArea2.setText(valorResultado);
            
            
            //}catch(Exception e){
            // e.printStackTrace();
            //}
        } catch (Exception ex) {
            Logger.getLogger(TraductorJava.class.getName()).log(Level.SEVERE, null, ex);
        }
         //txtArea2.setText(txtArea1.getText());
        
    }                                           

